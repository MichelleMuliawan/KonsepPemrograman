#include <stdio.h>

int main()
{
    int y, x;
    printf ("Masukkan x = ");
    scanf ("%d", &x);
    printf ("Masukkan y = ");
    scanf ("%d", &y);
    if ( y == 8) 
    if ( x == 5 ) 
    printf("@@@@@\n"); 
    else {
    printf ( "#####\n"); 
    printf ( "$$$$$\n");
    printf ( "&&&&&\n");
    }

    return 0;
}
