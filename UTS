#include <stdio.h>

int main()
{
    char jenis;
    int hargaa = 7650;
    int hargab = 11150;
    int jumlah, harga, diskon;
    printf ("Pertalite = A\n");
    printf ("Pertamax = B\n");
    printf ("Jenis bensin = ");
    scanf ("%c", &jenis);
    printf ("Jumlahh bensin = ");
    scanf ("%d", &jumlah);

    switch (jenis)
    {   case 'A': 
        harga = jumlah *  hargaa;
        printf("Harga %d", harga); break;
        if (jumlah >= 150000)
        total = 0.99 * harga;
        else if (jumlah >= 200000)
        total = 0.98 * harga;
        else if (jumlah >= 250000)
        total = 0.97 * harga;
        case 'B': 
        harga = jumlah * hargab;
        printf("Harga %d", harga); break;
        if (jumlah >= 300000)
        total = 0.98 * harga;
        else if (jumlah >= 400000)
        total = 0.97 * harga;
        else if (jumlah >= 500000)
        total = 0.96 * harga;
        default : printf("Tidak Valid\n");
    }
    
    return 0;
}

