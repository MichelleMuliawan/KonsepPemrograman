#include <stdio.h>

int main()
{
    int a, b, c, d, e, z;
    printf (" Masukkan a = ");
    scanf ("%d", &a);
    printf (" Masukkan b = ");
    scanf ("%d", &b);
    printf (" Masukkan c = ");
    scanf ("%d", &c);
    printf (" Masukkan d = ");
    scanf ("%d", &d);
    printf (" Masukkan e = ");
    scanf ("%d", &e);
    z=a<b? a:b;
    z=c<min? c:min;
    z=d<min? d:min;
    z=e<min? e:min;
    printf ("%d", z);
    
    return 0;

}

